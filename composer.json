{
  "name": "zapalm/request-proxy",
  "type": "library",
  "description": "The simple PHP-library to proxy HTTP-requests just having CURL.",
  "keywords": [
    "proxy-client",
    "proxy-server",
    "CURL",
    "requests",
    "php-library"
  ],
  "homepage": "https://github.com/zapalm/request-proxy",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim T.",
      "email": "zapalm@yandex.com",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/zapalm/curl-helper"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "zapalm/curl-helper": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "zapalm\\requestProxy\\classes\\": "classes",
      "zapalm\\requestProxy\\controllers\\": "controllers"
    }
  }
}